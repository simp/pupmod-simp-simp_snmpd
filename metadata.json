{
  "name": "simp-simp_snmpd",
  "version": "1.0.0",
  "author": "SIMP Team",
  "summary": "Configures snmpd agent using v3 USM on the client",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_snmpd",
  "project_page": "https://github.com/simp/pupmod-simp-simp_snmpd",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "snmp",
    "mib"
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    },
    {
      "name": "puppet/snmp",
      "version_requirement": ">= 5.1.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.0 < 7.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/iptables",
        "version_requirement": ">= 6.0.0 < 7.0.0"
      },
      {
        "name": "simp/logrotate",
        "version_requirement": ">= 6.5.0 < 7.0.0"
      },
      {
        "name": "simp/rsync",
        "version_requirement": ">= 6.0.0 < 7.0.0"
      },
      {
        "name": "simp/rsyslog",
        "version_requirement": ">= 7.6.0 < 8.0.0"
      },
      {
        "name": "simp/tcpwrappers",
        "version_requirement": ">= 6.2.0 < 7.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ]
}
